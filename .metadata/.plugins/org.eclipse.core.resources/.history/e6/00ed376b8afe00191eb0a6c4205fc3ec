package model;

public class Artist {
	private String id;
	private String name;
	private String role;
	
	
	
	public Artist(String id, String name) {
		this.id = id;
		this.name = name;
	}
	
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		String returnData = name.substring(0, 1).toUpperCase() + name.substring(1);
		return returnData;
	}
	public void setName(String name) {
		this.name = name;
	}
}
